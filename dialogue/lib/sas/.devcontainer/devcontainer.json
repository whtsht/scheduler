{
    "name": "rust remote development",
    "dockerComposeFile": "compose.yml",
    "service": "app",
    "workspaceFolder": "/app",
    "remoteUser": "vscode",
    "customizations": {
        "vscode": {
            "extensions": [
                "EditorConfig.EditorConfig",
                "rust-lang.rust-analyzer"
            ]
        }
    }
}