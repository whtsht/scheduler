{
    "name": "python remote development",
    "dockerComposeFile": "compose.yml",
    "service": "app",
    "workspaceFolder": "/app",
    "remoteUser": "vscode",
    "customizations": {
        "vscode": {
            "extensions": [
                "EditorConfig.EditorConfig",
                "ms-python.python",
                "ms-python.flake8",
                "ms-python.autopep8",
                "njpwerner.autodocstring"
            ]
        }
    }
}